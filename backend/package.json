{
  "name": "nestjs-boilerplate",
  "version": "1.0.0",
  "description": "A clean NestJS boilerplate with TypeScript, MongoDB, JWT authentication, and Swagger documentation",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier-standard '{src,apps,libs,test}/**/*.{ts,js,json,html,css}'",
    "start": "nest start",
    "start:render": "node --max-old-space-size=512 dist/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "seed": "ts-node -r tsconfig-paths/register src/seed.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand --config ./jest.config.js test/unit && jest --runInBand --config ./jest.config.js test/integration && jest --runInBand --config ./test/jest-e2e.config.js",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --config ./jest.config.js",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --runInBand --config ./test/jest-e2e.config.js",
    "test:unit": "jest --runInBand --config ./jest.config.js test/unit",
    "test:int": "jest --runInBand --config ./jest.config.js test/integration",
    "commit": "git-cz"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/common": "11.0.11",
    "@nestjs/config": "4.0.1",
    "@nestjs/core": "11.0.11",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mongoose": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "11.0.11",
    "@nestjs/platform-socket.io": "^11.0.11",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "11.0.6",
    "@nestjs/testing": "11.0.11",
    "@nestjs/websockets": "^11.0.11",
    "@types/bcrypt": "^5.0.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "axios": "^1.13.2",
    "bcrypt": "5.1.1",
    "bcryptjs": "^3.0.2",
    "buffer": "6.0.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "dotenv": "^16.4.7",
    "express": "^5.2.1",
    "ini": "^5.0.0",
    "inquirer": "^12.4.3",
    "ioredis": "^5.6.0",
    "module-alias": "2.2.3",
    "mongodb": "6.14.2",
    "mongoose": "^8.12.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "rxjs": "^7",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "5.0.1"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^1.2.0",
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "11.0.2",
    "@types/express": "5.0.0",
    "@types/ini": "^4.1.1",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "22.13.10",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/superagent": "8.1.9",
    "@types/supertest": "^6.0.2",
    "cz-conventional-changelog": "3.3.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^11.0.1",
    "prettier": "3.5.3",
    "prettier-standard": "^16.4.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.8.2"
  },
  "jest": {
    "moduleNameMapper": {
      "^@shared(.*)$": "<rootDir>/components/shared$1",
      "^@components(.*)$": "<rootDir>/components$1",
      "^@filters(.*)$": "<rootDir>/filters$1",
      "^@guards(.*)$": "<rootDir>/guards$1",
      "^@interfaces(.*)$": "<rootDir>/interfaces$1",
      "^@pipes(.*)$": "<rootDir>/pipes$1",
      "^@dto(.*)$": "<rootDir>/dto$1",
      "^@__types__(.*)$": "<rootDir>/__types__$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "_moduleAliases": {
    "@shared": "./dist/components/shared",
    "@components": "./dist/components",
    "@filters": "./dist/filters",
    "@guards": "./dist/guards",
    "@interfaces": "./dist/interfaces",
    "@pipes": "./dist/pipes",
    "@dto": "./dist/dto",
    "@__types__": "./dist/__types__"
  }
}
